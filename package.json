{
  "name": "valley-guessr",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:1": "vite",
    "dev:2": "start http://localhost:5173",
    "build": "run-s build:*",
    "build:1": "node --import tsx scripts/generateTypes.ts",
    "build:2": "tsc",
    "build:3": "vite build",
    "lint": "run-s lint:*",
    "lint:1-eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:2-typescript": "tsc --project tsconfig.json --noEmit",
    "lint:3-mapLinks": "node --import scripts/mapLinkChecker.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mantine/core": "^7.11.2",
    "@mantine/hooks": "^7.11.2",
    "@swan-io/chicane": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-zoom-pan-pinch": "^3.6.1",
    "ts-pattern": "^5.2.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.4",
    "@stylistic/eslint-plugin": "^2.6.1",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.40",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.3",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-tsconfig-paths": "^4.3.2"
  }
}